@model NotesMarkePlace.Models.User

<html lang="en">

<head>
    <!-- Import meta tags -->
    <meta charset="utf-8">

    <!-- For making it compatible to run on internet explorer and windows edge -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <!-- Gives instruction to browser hoe to control device dimensions and scaling -->
    <meta name="viewport" content="width=device-width,inital-scale=1">

    <!-- Title -->
    <title> Notes MarketPlace </title>

    <!-- Basic SEO -->
    <meta name="description" content="Creative website development with html5, css3, jQuery & bootstrap framework">
    <meta name="keywords" content="HTML5, CSS3, JQuery, Bootstrap, Web Design, Web Development, Responsive Website, Creative Website">

    <!-- Favicon -->
    <link rel="shortcut icon" href="~/images/Homepage/favicon.ico">

    <!-- Google Fonts -->
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@300;400;600;700&display=swap" rel="stylesheet">

    <!-- Fontawesome -->
    <link rel="stylesheet" href="~/Content/font-awesome.min.css">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/Content/bootstrap.min.css">

    <!-- Custom CSS -->
    <link rel="stylesheet" href="~/Content/style.css>

    <!-- Responsive Tabs CSS -->
    <link rel="stylesheet" href="~/Content/responsive.css">


</head>


<body data-spy="scroll" data-target=".navbar" data-offset="65">

    <!-- Header -->
    <header>
        <nav class="navbar navbar-fixed-top">
            <div class="container-fluid">
                <div class="site-nav-wrapper">

                    <div class="navbar-header">
                        <!-- Mobile Menu Open Button -->
                        <span id="mobile-nav-open-btn">&#9776;</span>

                        <!-- Logo -->
                        @Html.ActionLink("Home", "index", null, new { @class = "navbar-brand smooth-scroll" })
                        <img src="~/images/Pre-Login/logo.png" class="img-responsive" alt="logo">

                    </div>

                    <!-- Main Menu -->
                    <div class="container">
                        <div class="collapse navbar-collapse">
                            <ul class="nav navbar-nav pull-right">
                                <li>@Html.ActionLink("Search Notes", "searchNotes", null, new { @class = "smooth-scroll" })<hr /></li>
                                <li>@Html.ActionLink("Sell Your Notes", "Dashboard", null, new { @class = "smooth-scroll" })<hr /></li>
                                <li>@Html.ActionLink("FAQ", "FAQ", null, new { @class = "smooth-scroll" })<hr /></li>
                                <li>@Html.ActionLink("Contact Us", "ContactUs", null, new { @class = "smooth-scroll" })<hr /></li>
                                <li>@Html.ActionLink("Login", "login", null, new { @id = "login-btn", @class = "btn btn-blue", @title = "Login", @role = "button" })</li>
                            </ul>
                        </div>
                    </div>

                    <!-- Mobile Menu -->
                    <div id="mobile-nav">
                        <!-- Mobile Menu Close Button -->
                        <span id="mobile-nav-close-btn">&times;</span>

                        <div id="mobile-nav-content">
                            <ul class="nav">
                                <li>@Html.ActionLink("Search Notes", "searchNotes", null, new { @class = "smooth-scroll" })</li>
                                <li>@Html.ActionLink("Sell Your Notes", "Dashboard", null, new { @class = "smooth-scroll" })</li>
                                <li>@Html.ActionLink("FAQ", "FAQ", null, new { @class = "smooth-scroll" })</li>
                                <li>@Html.ActionLink("Contact Us", "ContactUs", null, new { @class = "smooth-scroll" })</li>
                                <li>@Html.ActionLink("Login", "login", null, new { @id = "login-btn", @class = "btn btn-blue", @title = "Login", @role = "button" })</li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </nav>
    </header>
    <!-- Header Ends -->
    <!-- Home -->
    <section id="home">

        <div class="content-box-lg">

            <div class="cotainer">

                <div class="row">

                    <div class="col-md-12 wow fadeIn">

                        <div id="home-statement" class="text-left">

                            <h4>Download Free/Paid Notes <br> or Sale your Book</h4>

                            <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit.<br>Error hic quidem, optio eligendi dolores consequatur.</p>

                        </div>

                        <div id="home-btn">
                            @Html.ActionLink("Learn More", "FAQ", null, new { @class = "btn btn-general btn-home smooth-scroll", @title = "Learn More", @role = "button" })
                        </div>

                    </div>
                </div>


            </div>


        </div>

    </section>
    <!-- Home Ends -->
    <!-- About -->
    <section id="about">

        <div class="content-box-md">

            <div class="container">

                <div class="row">

                    <!-- About Left Side -->
                    <div class="col-md-6 col-sm-6 wow slideInLeft" data-wow-duration="1s">
                        <div id="about-left">

                            <div class="horizontal-heading">
                                <h5>About <br> NotesMarketPlace</h5>
                            </div>
                        </div>
                    </div>

                    <!-- About Right Side -->
                    <div class="col-md-6 col-sm-6 wow slideInRight" data-wow-duration="1s">

                        <div id="about-right">

                            <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Saepe sint obcaecati modi quos numquam, dolore aliquid. Facilis deserunt architecto corporis accusantium ea illum fugiat expedita, quam, dolorem inventore necessitatibus non.</p> <br>

                            <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Quaerat optio, dicta cupiditate veritatis error rem tempore voluptatum veniam ex quam! Quia sapiente sint suscipit iusto, cupiditate quas aperiam quam eaque.</p>

                        </div>

                    </div>

                </div>
            </div>

        </div>


    </section>
    <!-- About Ends -->
    <!-- Users -->
    <section id="users" class="text-center">
        <div class="content-box-md">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">

                        <div id="users-heading">
                            <div class="horizontal-heading">
                                <h5> How it Works</h5>
                            </div>
                        </div>

                        <div id="users-bottom">
                            <div class="col-md-6 text-center">
                                <div class="image img-circle text-center">
                                    <img class="circle" src="~/images/Homepage/download.png" alt="download">
                                </div>
                                <h3>Download Free/Paid Notes</h3>
                                <h4>Get Material for your <br> Course etc.</h4>
                                <div id="users-btn-downloads">
                                    @Html.ActionLink("Download", "searchNotes", null, new { @class = "btn btn-blue", @title = "Download", @role = "button" })
                                </div>
                            </div>

                            <div class="col-md-6">
                                <div class="image img-circle">
                                    <img src="~/images/Homepage/seller.png" alter="seller" class="circle">
                                </div>
                                <h3>Seller</h3>
                                <h4>Upload and Download Course <br> and Materials etc.</h4>
                                <div id="users-btn-seller">
                                    @Html.ActionLink("Sell Book", "Dashboard", null, new { @class = "btn btn-blue", @title = "Sell Book", @role = "button" })
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </section>
    <!-- Users Ends-->
    <!-- Customers Feedback -->
    <section id="customers-feedback">
